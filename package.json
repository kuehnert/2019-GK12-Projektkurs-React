{
  "name": "sokrates-frontend",
  "version": "1.0.10",
  "private": true,
  "scripts": {
    "clean": "rimraf build/{*,.*}",
    "clear": "rimraf node_modules/{*,.*} build/{*,.*} package-lock.json",
    "clear:build": "npm run clear && npm install && react-scripts build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "deploy:staging": "npm run build && rsync --archive --partial --compress --delete ./build/ deploy@marienschule.com:/home/deploy/sites/sokrates-staging --itemize-changes",
    "deploy:staging:v": "rsync --verbose --human-readable --archive --partial --progress --compress --delete ./build/ deploy@marienschule.com:/home/deploy/sites/sokrates-staging --itemize-changes --dry-run"
  },
  "dependencies": {
    "@date-io/date-fns": "2.0.1",
    "@fortawesome/fontawesome-free": "5.12.0",
    "@material-ui/core": "4.8.2",
    "@material-ui/icons": "4.5.1",
    "@material-ui/pickers": "3.2.10",
    "@reduxjs/toolkit": "1.2.3",
    "axios": "0.19.2",
    "date-fns": "2.9.0",
    "formik-material-ui": "2.0.0-alpha.3",
    "formik": "2.1.2",
    "material-table": "1.56.1",
    "papaparse": "5.1.1",
    "react-beautiful-dnd": "12.2.0",
    "react-dnd-html5-backend": "10.0.2",
    "react-dnd": "10.0.2",
    "react-dom": "16.12.0",
    "react-redux": "7.1.3",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.3.0",
    "react": "16.12.0",
    "redux-thunk": "2.3.0",
    "redux": "4.0.5",
    "typescript": "3.7.3"
  },
  "devDependencies": {
    "@types/classnames": "2.2.9",
    "@types/lodash": "4.14.149",
    "@types/material-ui": "0.21.7",
    "@types/node": "13.5.0",
    "@types/papaparse": "5.0.3",
    "@types/react-beautiful-dnd": "11.0.4",
    "@types/react-dom": "16.9.5",
    "@types/react-redux": "7.1.7",
    "@types/react-router-dom": "5.1.3",
    "@types/react": "16.9.19",
    "@types/webpack-env": "1.15.0"
  },
  "quokka": {
    "babel": {
      "ts": true,
      "presets": [
        "@babel/preset-react"
      ]
    },
    "plugins": [
      "jsdom-quokka-plugin"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
